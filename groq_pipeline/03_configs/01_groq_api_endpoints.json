{
  "base_urls": {
    "docs_base": "https://console.groq.com/docs",
    "production_models": "https://console.groq.com/docs/models#production-models",
    "production_systems": "https://console.groq.com/docs/models#production-systems",
    "rate_limits": "https://console.groq.com/docs/rate-limits",
    "model_details_template": "https://console.groq.com/docs/model/{model_id}",
    "api_keys": "https://console.groq.com/keys"
  },
  "timeouts": {
    "page_load": 15,
    "element_wait": 10,
    "request_timeout": 30,
    "selenium_implicit": 5,
    "retry_delay": 3,
    "max_retries": 5,
    "model_scraping_delay": 1
  },
  "chrome_options": [
    "--headless",
    "--no-sandbox",
    "--disable-dev-shm-usage",
    "--disable-gpu",
    "--disable-web-security",
    "--allow-running-insecure-content",
    "--window-size=1920,1080",
    "--user-agent=Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
  ],
  "selenium_settings": {
    "webdriver_manager_cache": true,
    "page_load_strategy": "normal",
    "implicit_wait": 5,
    "script_timeout": 30
  },
  "scraping_patterns": {
    "table_selector": "table",
    "header_row_selector": "tr:first-child",
    "data_row_selector": "tr:not(:first-child)",
    "cell_selectors": ["td", "th"]
  },
  "rate_limiting": {
    "requests_per_minute": 60,
    "concurrent_requests": 3,
    "backoff_multiplier": 2,
    "respect_robots_txt": true
  }
}