# Pipeline Database Connection (PostgreSQL - pipeline_writer role)
# This connection string is used by all pipeline scripts for read-write access
# Format: postgresql://pipeline_writer:PASSWORD@db.PROJECT_REF.supabase.co:5432/postgres
PIPELINE_SUPABASE_URL=postgresql://pipeline_writer:YOUR_PASSWORD_HERE@db.YOUR_PROJECT_REF.supabase.co:5432/postgres

# Vercel App (Public - anon key)
# These are used only by the Vercel frontend app (browser-side queries)
# RLS policies restrict anon key to read-only access
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
SUPABASE_ANON_KEY=YOUR_ANON_KEY_HERE

# API Keys (if needed)
GROQ_API_KEY=YOUR_GROQ_API_KEY_HERE
