{
  "special_name_rules": {
    "gemma_3n_rule": {
      "description": "Gemma 3N should be written as Gemma 3n (lowercase n)",
      "pattern": "gemma 3n",
      "case_sensitive": false,
      "replacement": "Gemma 3n",
      "priority": 1
    }
  },
  "case_corrections": {
    "n_suffix": {
      "description": "N suffix in Gemma 3N should be lowercase",
      "patterns": [
        "Gemma 3N",
        "gemma 3N", 
        "GEMMA 3N"
      ],
      "replacement": "Gemma 3n"
    }
  },
  "validation_rules": {
    "gemma_naming": {
      "valid_formats": [
        "Gemma 2",
        "Gemma 3",
        "Gemma 3n"
      ],
      "invalid_formats": [
        "Gemma 3N",
        "gemma 3N"
      ]
    }
  }
}